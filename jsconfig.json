{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"Constants/*": ["src/_constants/*"],
			"Assets/*": ["src/_assets/*"],
			"Utils/*": ["src/_utils/*"],
      "Lib/*": ["src/_libs/*"],
			"@/*": ["src/*"],
			"@@/*": ["src/presentations/*"],
      "Components/*": ["src/presentations/components/*"]
		},
		"module": "commonjs",
		"target": "es6"
	},
	"include": ["src/**/*", "server.js"],
	"exclude": ["node_modules"]
}
